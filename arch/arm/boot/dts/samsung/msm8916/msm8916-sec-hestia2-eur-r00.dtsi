#include "msm8916-pinctrl-sec-hestia2-eur-r00.dtsi"
#include "msm8916-camera-sensor-hestia2-r00.dtsi"
#include "msm8916-sec-hestia2-eur-battery-r00.dtsi"
#include "../../../../../../drivers/video/msm/mdss/samsung/EA8061V_AMS497EE01/dsi_panel_EA8061V_AMS497EE01_hd_octa_video.dtsi"
